<div class="container mt-5 mb-5 d-flex flex-column align-items-center">
    <h1 class="display-1" id="login-header">Log In</h1>
    <h4 class="text-danger mb-3">Unsuccessful log in. Please try again or register.</h4>
    <form action="/admin/loginAgain" method="POST">
        <div class="mb-3">
            <label for="exampleInputEmail1" class="form-label">Email address</label>
            <input type="email" class="form-control" id="email" name="email" aria-describedby="emailHelp">
            <div id="emailHelp" class="form-text">We'll never share your email with anyone else.</div>
        </div>
        <div class="mb-3">
            <label for="exampleInputPassword1" class="form-label">Password</label>
            <input type="password" class="form-control" id="password" name="password">
        </div>
        <button type="submit" class="btn btn-primary">Submit</button>
    </form>
</div>
<div class="mx-auto" style="width: 200px;">
  <a href="/admin/register" style="font-size: 35px;">Register!</a>
</div>